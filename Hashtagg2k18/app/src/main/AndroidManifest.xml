<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ar.developments.hashtagg2k18">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QR_code_reader" android:screenOrientation="portrait"> </activity>
        <activity android:name=".Registration_Portal" android:screenOrientation="portrait" ></activity>
        <activity android:name=".Co_ordinator_Portal" android:screenOrientation="portrait"></activity>
        <activity android:name=".ListView_participants" android:screenOrientation="portrait"></activity>
    </application>

</manifest>